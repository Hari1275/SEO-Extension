{
  "manifest_version": 3,
  "name": "Advanced SEO Analyzer",
  "version": "1.1",
  "description": "Analyzes basic SEO elements of the current webpage with enhanced features",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["chart.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
